{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/favicon.png",
      "destination": "/api/favicon.js"
    },
    {
      "source": "/favicon.ico",
      "destination": "/api/favicon.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

